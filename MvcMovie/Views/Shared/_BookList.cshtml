@model MvcMovie.Models.ViewBooksRowModel


<div class="card m-2">
    <div class="card-header">
        @Model.Title
    </div>
    <div class="card-body pt-0 px-1 container-sm" >
        @foreach (var book in Model.Books.Take(3))
        {
            @await Html.PartialAsync("_Book", book)
        }
    </div>
</div>